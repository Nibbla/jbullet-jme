                        <property>run.class</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="javadoc">
                    <target>javadoc</target>
                </action>
            </ide-actions>
            <export>
                <type>jar</type>
                <location>dist/bullet-jme.jar</location>
                <build-target>dist</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>build/classes</location>
                <build-target>dist</build-target>
            </export>
            <view>
